{
	"host": "localhost",
	"port": 3000,
	"apiHost": "localhost",
	"apiPort": 3030,
	"isProdcution": false,
	"app": {
		"title": "MStar SES",
		"description": "School expert system.",
		"head": {
			"titleTemplate": "%s - MStar SES",
			"meta": [
				{ "name": "description", "content": "School expert system." },
				{ "charset": "utf-8" },
				{ "property": "og:site_name", "content": "MStar SES" },
				{ "property": "og:image", "content": "https://react-redux.herokuapp.com/logo.jpg" },
				{ "property": "og:locale", "content": "en_US" },
				{ "property": "og:title", "content": "MStar SES" },
				{ "property": "og:description", "content": "School expert system." },
				{ "property": "og:card", "content": "summary" },
				{ "property": "og:image:width", "content": "200" },
				{ "property": "og:image:height", "content": "200" }
			]
		}
	},
	"api": {
		"auth": {
			"service": "user",
			"secret": "mstar-ses-super-secret",
			"cookie": {
				"enabled": true,
				"httpOnly": false,
				"maxAge": 86400000,
				"secure": false
			}
		},
		"authLocal": {
			"service": "user",
			"usernameField": "omVNumber",
			"entityUsernameField": "omVNumber"
		},
		"authJwt": {
			"service": "user"
		},
		"postgres": {
			"client": "pg",
			"connection": "postgres://postgres:postgres@localhost:5432/ses"
		}
	}
